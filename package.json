{
  "name": "@unsync/grdf-client",
  "type": "module",
  "version": "1.1.20",
  "license": "MIT",
  "repository": "unsync/grdf-client",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx src/run.ts",
    "build": "tsc",
    "prepack": "npm run build",
    "checkdeps": "ncu",
    "checkdeps:apply": "ncu -u",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "config:pull": "npx dotenv-vault pull development",
    "config:push": "npx dotenv-vault push development",
    "release:major": "npm version -m 'release:major' major",
    "release:minor": "npm version -m 'release:minor' minor",
    "release:patch": "npm version -m 'release:patch' patch",
    "postversion": "git push origin HEAD && git push origin v$npm_package_version",
    "test": "exit 0"
  },
  "dependencies": {
    "@okta/okta-auth-js": "^7.12.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "fetch-cookie": "^3.1.0",
    "tough-cookie": "^5.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "eslint": "^9.34.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.18",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix . --no-warn-ignored"
  }
}
