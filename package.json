{
  "name": "@unsync/grdf-client",
  "type": "module",
  "version": "1.0.4",
  "license": "MIT",
  "repository": "unsync/grdf-client",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "checkdeps": "ncu",
    "checkdeps:apply": "ncu -u",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "release:major": "npm version -m 'release:major' major",
    "release:minor": "npm version -m 'release:minor' minor",
    "release:patch": "npm version -m 'release:patch' patch",
    "test": "exit 0"
  },
  "dependencies": {
    "@unsync/nodejs-tools": "^1.0.4",
    "dayjs": "^1.11.10",
    "html-to-text": "^9.0.5",
    "puppeteer": "^21.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.13",
    "@unsync/eslint": "^1.0.22",
    "@unsync/tsconfig": "1.0.12",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "npm-check-updates": "^16.14.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": "eslint --fix ."
  }
}
